{% extends 'base.html.twig' %}

{% block title %}Association Linas Autrement, galerie{% endblock %}

{% block body %}

    <!--Page Title-->
    <section class="page-title" style="background-image:url(/images/background/7.jpg);">
        <div class="auto-container">
            <h1>Gallery</h1>
            <ul class="bread-crumb clearfix">
                <li><a href="{{ path('home') }}">Home </a></li>
                <li>Gallery</li>
            </ul>
        </div>
    </section>
    <!--End Page Title-->

    <!-- Gallery Section -->
    <section class="gallery-section">
        <!--MixitUp Galery-->
        <div class="mixitup-gallery">

            <div class="filter-list row clearfix">

                {% for gallery in galleries %}
                    <!-- Gallery Item -->
                    <div class="gallery-item mix all marketing analys col-lg-2 col-md-6 col-sm-6 col-xs-12">
                        <div class="inner-box">
                            <figure class="image"><img src="{{ gallery.url }}" alt="{{ gallery.caption }}"></figure>
                            <div class="caption-box">
                                <div class="inner">
                                    <div class="title"><h3><a href="#">Linas autrement</a></h3></div>
                                    <h5>{{ gallery.caption }}</h5>
                                    <div class="icon-box">
                                        <a href="{{ gallery.url }}" class="lightbox-image" data-fancybox="Gallery"><span class="flaticon-add"></span></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endfor %}

            </div>
        </div>
    </section>
    <!-- End Gallery section -->


{% endblock %}

{% block javascripts %}
    <script src="/js/isotope.js"></script>
    <script src="/js/mixitup.js"></script>
    <script src="/js/jquery.fancybox.js"></script>
    <script src="/js/owl.js"></script>
    <script src="/js/wow.js"></script>
    <script src="/js/appear.js"></script>
    <script src="/js/script.js"></script>
{% endblock %}
