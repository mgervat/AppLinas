                        <li><i class="menu-icon fa fa-list-ul"></i><a href="{{ path('members') }}">Liste des membres</a></li>
                        <li><i class="menu-icon fa fa-sign-in"></i><a href="{{ path('member_article', {'id':user.id}) }}">Vos Articles</a></li>
                        <li><i class="menu-icon fa fa-sign-in"></i><a href="#">Votre compte</a></li>
                        <li><i class="menu-icon fa fa-paper-plane"></i><a href="{{ path('account_logout') }}">Logout</a></li>
                    </ul>
                </li>
            </ul>
        </div><!-- /.navbar-collapse -->
    </nav>
</aside><!-- /#left-panel -->

